{
  "expo": {
    "name": "Books",
    "slug": "Books",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.books.app",
      "appleTeamId": "5383A4DQX6"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#160920"
      },
      "package": "com.anonymous.Books"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-splash-screen",
        {
          "backgroundColor": "#160920",
          "image": "./assets/splash-icon.png",
          "dark": {
            "image": "./assets/splash-icon.png",
            "backgroundColor": "#160920"
          },
          "imageWidth": 200
        }
      ],
      [
        "expo-font",
        {
          "fonts": ["./assets/fonts/Georgia.ttf", "./assets/fonts/NunitoSans.ttf"]
        }
      ]
    ]
  }
}
